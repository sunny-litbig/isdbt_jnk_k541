#	Make All Function 
#########################################################
all :

#########################################################
#	Make Install Function 
#########################################################
install : 
	cp -a $(LOCAL_TARGET)$(STATIC) $(COPY_TO_LIB_PATH)


#########################################################
#	Make Clean Function 
#########################################################	
clean : 
